{
  "name": "officehours-node",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www --ignore vue",
    "start": "NODE_ENV=production node ./bin/www",
    "reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "test": "mocha --require test/hooks.js --recursive --timeout 2000 --exit",
    "cov": "nyc --reporter=html mocha --require test/hooks.js --recursive --timeout 2000 --exit",
    "test-json": "mocha --require test/hooks.js --recursive --timeout 2000 --exit --reporter json > test-results.json"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.4.5",
    "express": "4.18",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-cas-authentication": "github:alt-cs-lab/cas-authentication",
    "objection": "^3.1.4",
    "pug": "^3.0.2",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1",
    "socket.io-client": "^4.7.4"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3"
  }
}
